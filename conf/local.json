{
  "services": {
    "CoAuthoring": {
      "sql": {
        "type": "postgres",
        "dbHost": "127.0.0.1",
        "dbPort": "5432",
        "dbName": "__DB_NAME__",
        "dbUser": "__DB_USER__",
        "dbPass": "__DB_PAD__"
      },
      "token": {
        "enable": {
          "request": {
            "inbox": true,
            "outbox": true
          },
          "browser": true
        },
        "inbox": {
          "header": "Authorization"
        },
        "outbox": {
          "header": "Authorization"
        }
      },
      "secret": {
        "inbox": {
          "string": "__JWT_SECRET__"
        },
        "outbox": {
          "string": "__JWT_SECRET__"
        },
        "session": {
          "string": "__JWT_SECRET__"
        }
      }
    }
  },
  "rabbitmq": {
    "url": "amqp://guest:guest@localhost"
  }
}
